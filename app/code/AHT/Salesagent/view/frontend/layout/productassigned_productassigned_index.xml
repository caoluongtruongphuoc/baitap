<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">List Product Assigned</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block 
                class="AHT\Salesagent\Block\Frontend\ListProductAssigned" 
                name="list-product-assigned-table" 
                template="AHT_Salesagent::list-product-assigned-table.phtml" 
                cacheable="false"
            />
        </referenceContainer>
    </body>
</page>